// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String @id @default(uuid())
  FName       String
  LName       String
  email       String @unique
  password    String @unique
  role        String
  cic         String[]

  @@map(name: "users")
}